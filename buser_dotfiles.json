{
  "link": {
    "bashrc": "~/.bashrc",
    "vimrc": "~/.vimrc",
    "emacs": "~/.emacs",
    "tmux.conf": "~/.tmux.conf",
    "zshrc": "~/.zshrc"
  },
  "pre_commands": [
    "curl -fLo ~/.vim/autoload/plug.vim --create-dirs https://raw.githubusercontent.com/junegunn/vim-plug/master/plug.vim",
    "git clone --depth 1 https://github.com/junegunn/fzf.git ~/.fzf",
    "~/.fzf/install"
  ],
    "commands": [
      "vim +PlugInstall"
  ]
}
